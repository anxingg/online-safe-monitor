<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 安全生产事故模块 -->
    <package name="safeaccident" namespace="/safeaccident" extends="struts-default">
    
    	<!-- 没有返回页面 -->
        <action name="safeaccident_*_*" class="cn.com.wh.safeaccident.action.SafeAccident{1}Action" method="{2}">
        </action>
    
    	<!-- 有返回页面 -->
        <action name="safeaccidentload_*_*" class="cn.com.wh.safeaccident.action.SafeAccidentLoadAction" method="{1}">
        	<result name="success">/wh/logined/safeAccident/{2}.jsp</result>
        </action>
        
    </package>
    
</struts>
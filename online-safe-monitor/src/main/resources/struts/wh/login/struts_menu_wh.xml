<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
     <!-- 获取菜单 -->
    <package name="whmenu" namespace="/whmenu" extends="struts-default">
        <action name="fetchOSMIndexMenu" class="cn.com.wh.login.action.MenuAction" method="fetchOSMIndexMenu">
            <result name="menu">/wh/logined/osmIndexMenu.jsp</result>
        </action>
        <action name="fetchSubSystemMenu" class="cn.com.wh.login.action.MenuAction" method="fetchSubSystemMenu">
            <result name="menu">/wh/logined/osmSubSystemMenu.jsp</result>
        </action>
    </package>
</struts>
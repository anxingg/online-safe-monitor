<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 安全生产投入管理模块 -->
    <package name="fee" namespace="/fee" extends="struts-default">
    
    	<!-- 没有返回页面 -->
        <action name="fee_*_*" class="cn.com.wh.fee.action.Fee{1}Action" method="{2}">
        </action>

        <!-- 企业年度统计-->
        <action name="findComapnyYearStatistic" class="cn.com.wh.fee.action.FeeUsedStatisticAction" method="findComapnyYearStatistic">
        	<result name="success">/wh/logined/fee/statisticTable.jsp</result>
        </action>

    	<!-- 有返回页面 -->
        <action name="feeToPage_*_*_*" class="cn.com.wh.fee.action.Fee{1}Action" method="{2}">
        	<result name="success">/wh/logined/fee/{3}.jsp</result>
        </action>
    </package>
    
</struts>
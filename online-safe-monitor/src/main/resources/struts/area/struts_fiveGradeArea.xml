<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
    
<struts>
	<package name="fiveGradeArea"  namespace="/fiveGradeArea" extends="struts-default">
		<!-- 获得各级地址的action -->
		<action name="getAddress" class="cn.com.qytx.hotline.area.action.FiveGradeAreaAction" method="getAddress">
		</action>
		<!-- 新增地址的action -->
		<action name="addNewArea" class="cn.com.qytx.hotline.area.action.AddNewAreaAction" method="addNewArea">
		</action>
		<!-- 删除地址的action -->
		<action name="deleteArea" class="cn.com.qytx.hotline.area.action.DeleteAreaAction" method="deleteArea">
		</action>
		<!-- 修改地址的action -->
		<action name="modifyArea" class="cn.com.qytx.hotline.area.action.ModifyAreaAction" method="modifyArea">
		</action>
	</package>
</struts>
body,html{height:100%;}

#div_pannel{}
*{margin:0;padding:0;}
#div_tab{background:#fff url(images/t_tab_bg.gif) repeat-x 0 bottom;height:26px;padding:5px 15px 0;margin-bottom:10px;}
#div_tab li{float:left;text-align:center;position:relative;list-style:none;}

#div_tab li{background:url(images/t_tab_uselectbg.gif);height:25px;line-height:25px;margin-right:-1px;}
#div_tab li span{background:url(images/t_tab_uselectbg.gif);height:25px;line-height:25px;}

#div_tab li.crent{background:url(images/t_tab_selectbg.gif);margin-top:1px;height:25px; }
#div_tab li.crent span{background:url(images/t_tab_selectbg.gif);height:25px;line-height:25px;}

#div_tab li,#div_tab li.crent{color:#fff;background-repeat:no-repeat;background-position:0 0;}
#div_tab li span,#div_tab li.crent span{display:inline-block;padding:0 40px 0 15px;background-repeat:no-repeat;background-position:right bottom;}

#div_tab li .menua{color:#1d5672;font-size:14px; text-decoration:none;position:relative;}
#div_tab li.crent .menua,#div_tab li .menua:hover ,#div_tab li .on{font-weight:bold;}
#div_tab li .win_close,#div_tab li.crent .win_close{width:14px;height:14px;position:absolute;top:5px;right:20px;cursor:pointer;display:block;overflow:hidden;background:url(images/t_delete_ico.gif) no-repeat;}
#div_tab li .win_close{background-position:0 -14px;}
#div_tab li .win_close:hover{background-position:0 0;}

.clearfix:after {content:"."; display:block;height:0;clear:both; visibility:hidden;}
*html .clearfix{height:1%;}
*+html .clearfix{height:1%;}
.clearfix{display:inline-block;}
/* Hide from IE Mac */
.clearfix {display:block;}
/*tab scroll*/
.tabbox {width: 100%;height: 35px;background:url(../images/t_tab_bg.gif) repeat-x;}
#div_tab {white-space: nowrap;left: 0;position: absolute;top: 0;width: auto;z-index:99}
#div_tab li {white-space: nowrap;display: inline-block;float: inherit}
.tabList {height: 35px; position: relative;overflow:hidden;margin-right:30px;}
.tabbox .btnScroll { width:30px; position:absolute; right:-6px; top:10px; height:40px;display:block;z-index:999
}
.tabbox .btnScroll a{ display:block;float:Left; width:5px;margin-right:10px; height:10px; background:url(images/btnScroll.png) no-repeat;}
.tabbox .btnScroll a.btnLeft{ background-position:0px -10px;}
.tabbox .btnScroll a.btnLeftNo{background-position:0px 0px;}
.tabbox .btnScroll a.btnRight{ background-position:-14px 0px;}
.tabbox .btnScroll a.btnRightNo{ background-position:-14px -10px;}

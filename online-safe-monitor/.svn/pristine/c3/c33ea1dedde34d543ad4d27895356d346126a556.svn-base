/*bottom*/
.bottom{width: 100%; text-align: center;height:30px;line-height:30px; overflow:hidden; color:#fff; background: #0591aa; position: absolute; bottom: 0; left: 0;}
.bottom i{ color:#fff; margin:0 3px;}
.exBot{ padding-top:0px; padding-right:5px;}
.exBot em{ display:inline-block; width:22px; height:20px; display:inline-block; vertical-align:middle; background:url(../images/main/affairs.png) no-repeat; line-height:20px; cursor:pointer; margin-right:10px;}
.exBot em.icon_work{background-position:0 -81px;}
.exBot em.messageBox{background-position:0 -40px;}

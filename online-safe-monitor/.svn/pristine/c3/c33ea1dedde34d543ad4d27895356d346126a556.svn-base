#first_panel .scroll-up {background: url("../images/main/scroll_arrow.png") no-repeat scroll center 0; cursor: pointer;  height: 12px; overflow: hidden; margin-top:5px;}
#first_panel .scroll-down {background: url("../images/main/scroll_arrow.png") no-repeat center -10px;cursor: pointer; height: 10px; overflow: hidden;}
#first_panel .scroll-up:hover {background-position: center -20px}
#first_panel .scroll-down:hover {background-position: center -30px}


/*main ifame*/
.iframe_body{width: 100%; height: 100%; margin: 0; padding: 0; overflow: hidden;z-index:1}
.iframeFather {overflow-x: hidden;
	position:absolute;
	_position:relative;
	top:122px;
	_top: 0;
	left:0;
	bottom:30px;
	right:0;
	_overflow-x: hidden;
	overflow-y: auto;
	z-index:4;
	}
.iframeBox {position:absolute;
	_position: absolute;
	top:0px;
	left:0px;
	right: 0px;
	bottom:0px;
	overflow-x: hidden;
	overflow-y: auto;
	z-index:6;
}
.iframeBox iframe,.iframeContent {position: absolute; _position:relative;_overflow:hidden;}
.iframeBox iframe,.iframeContent {
	width: 100%;
	height: 100%;
	background-color: #fff;
	_height: expression(document.body.offsetHeight -170 + 'px');
	z-index:5;
	_overflow-x: hidden;
	_overflow-y: auto;

}
.iframeFather_cls{top:67px;_top:-10px;}
.iframeFather_cls iframe{_height: expression(document.body.offsetHeight - 95 + 'px');}


.exBot em.messageBox_on{ background:url(../images/main/jsxx.gif) no-repeat;}


/*OA换肤*/
.oa_hf{ width:359px; overflow:hidden; position:absolute; right:5px; z-index:10000; top:79px; display:none;}
.oa_hf .top_oh{ background:url(../images/main/top_hf.png) no-repeat; width:365px; height:13px; overflow:hidden; float:left;}
.oa_hf .cent_oh{ background:url(../images/main/cent_hf.png); width:337px; float:left;  padding:0 13px 0 9px;}
.oa_hf .cent_oh h3{ background:url(../images/main/fg_bj.png) repeat-x left bottom; height:40px; line-height:40px; text-align:center; color:#000; font-size:14px; font-weight:100; float:left; width:100%;}
.oa_hf .cent_oh ul{ width:100%; float:left;}
.oa_hf .cent_oh ul li{ padding-left:15px; width:150px; height:104px; float:left; padding-top:20px;}
.oa_hf .cent_oh ul li img{ width:150px; height:104px; cursor:pointer;}
.oa_hf .cent_oh .close_hf{ height:50px; background:url(../images/main/fg_bj.png) repeat-x left top;float:left; width:100%; margin-top:20px;}
.oa_hf .cent_oh .close_hf em{ float:right; background:url(../images/main/close_hf.png) no-repeat; width:68px; height:27px; cursor:pointer; margin-top:13px; margin-right:10px;}
.oa_hf .down_oh{ background:url(../images/main/down_hf.png) no-repeat; width:365px; height:6px; overflow:hidden;}
/*iframe*/
.iframeresize{width:100%;}

/*事务提醒-系统右下角*/
.work_pop{ position:absolute; background:url(../images/main/msgPop.png) no-repeat; width:259px; height:137px; overflow:hidden; z-index:1001; right:10px; bottom:30px; color:#333; display:none;}
.work_pop h2{ height:28px; line-height:28px; text-indent:40px; margin-top:6px;}
.work_pop h2 .close{cursor: pointer;float: right;height: 20px;margin-right: 6px; margin-top: 1px;  width: 20px;}
.work_pop p.msg{ padding:16px 20px;}
.work_pop p.more{height: 34px;line-height: 32px;margin-top: 7px;text-align: right; padding:0 10px;}

/*事务提醒-弹窗*/
.work_details{ padding:10px;}
.work_details h2{ height:30px; line-height:30px;}
.work_details h2 span{ color:#888;}
.work_details h2 a{ float:right; line-height:30px;}
.work_details dl dt{ height:27px; line-height:27px; color:#333; padding-left:10px; padding-right:25px; background:#ecf5f7 url(../images/main/work_icon.png) no-repeat right center;}
.work_details dl dd{ border-bottom:1px solid #e8eaeb; color:#555555; padding:8px 20px;}
.work_details dl dd p{ line-height:25px;}
.work_details dl dd p .day{ float:right; line-height:25px; color:#ff6600;}

/*没有数据*/
.no_data{background: url("../images/main/no_data.png") no-repeat;height: 70px;margin: 130px auto 0;padding-left: 80px; width: 170px;}
.no_data dt{font-size:20px; line-height:30px; color:#555555; padding-top:5px;}
.no_data dd{ color:#adadad; line-height:25px;}
.no_data dd .time{ color:#F00; padding:0 5px;}


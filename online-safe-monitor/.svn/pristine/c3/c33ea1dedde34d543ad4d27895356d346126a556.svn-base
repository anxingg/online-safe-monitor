@charset "utf-8";
/* CSS Document */
/*html,body {	padding: 0;	margin: 0;overflow: auto;overflow-x: hidden;width:100%;height:100%;}*/
html {height: 100%;_overflow-x: hidden;	_overflow-y: auto;}

/*head*/
.indexHeader {height: 122px; background:#0591aa;}
.indexHeaderCls{ height:60px;}
.indexHeaderCls .indexContent{ display:none;}
.indexHeaderCls #div_tab{ top:5px;}
.headbg{ width:100%;background:url(../images/main/headbg.png)no-repeat right top;}
.indexContent{height: 54px;overflow:hidden; }
.indexContent h1{height: 54px; margin: 0px 0 0 17px; line-height:54px; font-size:28px; color:#fff;}
.indexContent h1 img{ height:27px; margin-right:8px; float:left; margin-top:14px;}

.head_nav {color: #fff; position: absolute; top: 4px; right:0;}
.head_nav li{ float:left; padding-right:17px;}
.head_nav li p{ line-height:28px; font-size:12px; color:#FFF; text-align:right;}
.head_nav li p .time{font-size:27px;font-family:"Arial";font-weight:bold}
.head_nav li p.user{font-size:14px;}
.head_nav li p.user font{color:#edb11a;}
.head_nav li.last{ padding-right:10px;}

.menu_right{ float:right;height:35px; /*width:118px;*/}
.menu_right em{height: 35px; width: 23px;  text-align: center; display: block; color: #fefefe; float: left;background: url(../images/main/icon_head.png) no-repeat; cursor:pointer; margin:0 3px}
.menu_right em.cut{ background-position:center 10px;}/*切换*/
.menu_right em.fit{background-position:center -38px;}/*设置*/
.menu_right em.quit{background-position:center -70px;}/*注销*/
.menu_right em.clsHead{ background-position:center -103px;}/*收起*/
.menu_right em.opHead{ background-position:center -133px;}/*展开*/


/*tab*/
.index_nav { width:100%; line-height: 35px;height:35px;overflow:hidden}
.index_nav .index_menu{width: 85px; height:27px; line-height:27px; font-size:16px; font-weight:bold; color:#13496c; margin-left:6px; float:left;background:#fff;cursor:pointer; margin-top:4px; margin-right:8px; border-radius:3px;}
.index_nav .index_menu em{float:left; background:url(../images/main/home.png) no-repeat; width:21px; height:19px; display:inline;margin:4px 7px 0 9px;}

.index_nav .sy_nav{background: url(../images/sy_bj.png) no-repeat; width:79px; height:33px; float:left; margin-top:2px;}
.index_nav .divBox {display: block; float: left; margin-left: 20px;}
.index_nav div.current_nav {padding-left: 20px;  background: url(../images/navs_a_L.png) no-repeat; position: relative;}
.index_nav div.current_nav a{display:block; float: left; font-weight: bold; height:29px; line-height:29px; padding-right: 30px; background:url(../images/navs_a_R.png) no-repeat right bottom;}
.index_nav div.current_nav b.xx{display: block; width: 8px; height: 8px; background: url(../images/XX.png) no-repeat; position: absolute; right: 15px; top: 10px; cursor: pointer;}
.index_nav div.current_nav b.xx:hover {width: 7px; height: 7px; border: 1px solid #dedede;}
/*tab scroll*/
.index_nav .tabbox { float:left; height:30px;background:None; overflow:hidden; margin-top:5px;}
#taskbar_center{float:left;padding-top:2px;}

#tabs_container{white-space:nowrap;height:33px;overflow:hidden;float:left;}
#div_tab {white-space: nowrap;height:33px; float:left; width: auto;z-index:99}
#div_tab li{position:relative;text-align:center;float:left;margin:1px 3px 0 3px; display:inline; padding:0 10px; height:32px; border-radius:3px 3px 0 0;font-size:16px; line-height:32px; border:1px solid #03798c; border-bottom:none; background:#09899e;}
#div_tab li a{color:#FFF;}
#div_tab li#shouye {width:60px; padding-right:10px;}
#div_tab li.crent{ background:#f5f5f5; border:1px solid #ececec;border-bottom:none; padding:0 25px 0 10px;}
#div_tab li.crent a{color:#333;}
#div_tab li.crent .win_close{ background:url(../images/main/colse.png) no-repeat; width:10px; height:10px; position:absolute; right:10px; top:12px; cursor:pointer;}

.btnLeft,.btnRight { width:20px; height:30px;display:none;z-index:999; float:left;  cursor:pointer;}
.btnLeft a{ display:block; width:20px;  height:20px; background:url(../images/btnScroll_r.png) no-repeat 0px 2px; margin-top:5px;}
.btnLeft a:Hover{background-position:0px -18px;}
.btnRight a{ display:block;width:20px; height:20px;background:url(../images/btnScroll_l.png) no-repeat 0px 2px;  margin-top:5px;}
.btnRight a:hover{ background-position:0px -18px;}
.s_tab{ height:32px;line-height:32px; background:#f5f5f5; position:relative; border-bottom:1px solid #cccccc;}
.s_tab .lastMenu{ position:absolute;top:0px;/*left:10px;*/}
.s_tab li{height:32px;line-height:32px;float:left; overflow:hidden;margin:0 3px;}
.s_tab li a{ display:inline-block;height:18px; line-height:18px; padding:0 15px;color:#000;margin-top:6px;overflow:hidden; font-size:16px;}
.s_tab li a:hover{text-decoration: none}
.s_tab li.current{border-bottom:1px solid red;}
.s_tab li.current a{color:#ff0000;}
.s_tab .mCurrent{ position:absolute;top:0px;left:10px; display:block;}



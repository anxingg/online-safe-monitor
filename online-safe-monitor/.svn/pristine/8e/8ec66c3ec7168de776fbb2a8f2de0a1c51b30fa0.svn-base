package cn.com.qytx.workflow.service.impl;

import org.jbpm.api.JbpmException;

/**
 * Created by izerui.com on 14-5-6.
 */
public class WorkflowException extends JbpmException {

    /**
	 * 描述含义
	 */
	private static final long serialVersionUID = 6857925815808544921L;

	public WorkflowException() {
        super();
    }

    public WorkflowException(String msg) {
        super(msg);
    }

    public WorkflowException(Throwable cause) {
        super(cause);
    }

    public WorkflowException(String msg, Throwable cause) {
        super(msg, cause);
    }
}

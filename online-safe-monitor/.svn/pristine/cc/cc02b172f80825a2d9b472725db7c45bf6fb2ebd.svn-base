package cn.com.qytx.platform.domain;

import java.io.Serializable;

/**
 * @author Administrator
 * 语音服务器的参数设置
 */
public class VoxFileSet implements Serializable{
	
 /**
	 * 描述含义
	 */
	private static final long serialVersionUID = 1L;
//语音服务器的IP
  public  String voxHostIp;
	//语音服务端口
	public String voxPort;
	//录音文件路径
	public String voxPath;
	//参数描述
		public String parDescribe;
	//参数项
		public String parItems;
		public String getVoxHostIp() {
			return voxHostIp;
		}
		public void setVoxHostIp(String voxHostIp) {
			this.voxHostIp = voxHostIp;
		}
		public String getVoxPort() {
			return voxPort;
		}
		public void setVoxPort(String voxPort) {
			this.voxPort = voxPort;
		}
		public String getParDescribe() {
			return parDescribe;
		}
		public void setParDescribe(String parDescribe) {
			this.parDescribe = parDescribe;
		}
		public String getParItems() {
			return parItems;
		}
		public void setParItems(String parItems) {
			this.parItems = parItems;
		}
		public String getVoxPath() {
			return voxPath;
		}
		public void setVoxPath(String voxPath) {
			this.voxPath = voxPath;
		}
		
}

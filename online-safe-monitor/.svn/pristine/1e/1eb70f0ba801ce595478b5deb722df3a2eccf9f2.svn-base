@charset "utf-8";
/*外呼任务*/

/*页面步骤*/
.flow-steps {overflow: hidden;}
.flow-steps li {background:url(../images/icon/flow_steps_bg.png) no-repeat 100% 0px #e4e4e4; padding: 0px 24px 0px 0px; height: 40px; text-align: center; color: #888; line-height: 40px; overflow: hidden; font-size: 18px; float: left; width:304px;}
.flow-steps li span {display: block;}
.flow-steps li strong {display: block;}
.flow-steps li .first {background: url("../images/icon/flow_steps_bg.png") no-repeat -12px -69px rgb(228, 228, 228);}
.flow-steps li.last {background-position: 100% -240px;}
.flow-steps li.current {color:#FFF; background-color: rgb(255, 102, 0); font-weight:bold;}
.flow-steps li.current .first {background-position: -22px -160px; background-color: rgb(255, 102, 0);}
.flow-steps li.current-prev {background-position: 100% -40px !important;}
.flow-steps li.done {background-position: 100% -80px; color: rgb(255, 102, 0); background-color: rgb(255, 230, 186);}
.flow-steps li.done .first {background-position: -22px -200px; background-color: rgb(255, 230, 186);}
.flow-steps li.last-current {background-position: 100% -280px; color: rgb(255, 255, 255); background-color: rgb(255, 102, 0);}
.yz_qn{ overflow:hidden;}
.yz_qn .menu_left{float:left; width:130px; border:1px solid #e3e3e3; padding:6px; overflow:hidden; border-radius:5px; background:#f5f5f5;}
.yz_qn .menu_left h2{ font-size:20px; text-align:center;}
.yz_qn .menu_left ul li{ width:128px; height:35px; background:#FFF; border:1px solid #e3e3e3; overflow:hidden; line-height:35px; margin-top:10px; cursor:pointer;}
.yz_qn .menu_left ul li img{float: left; display:inline; margin:10px 10px 0 10px;}

.yz_qn .cont_right{float:right; width:100%;overflow:auto; background:#FFF; height:500px;}
.yz_qn .cont_right h2{ text-align:center;color:#000;font-size:20px; padding:10px 0;}
.yz_qn .cont_right .subject{overflow:hidden; color:#666; line-height:25px; text-indent:2em;}
.yz_qn .cont_right .buttonArea{ padding:15px 0; text-align:center; background:#f6f6f6; border-top:1px solid #dcdcdc;}
.yz_qn .cont_right .buttonArea em{ float:right; padding-right:15px;}
.yz_qn .cont_right .buttonArea em a{width:58px; height:22px; background:#edf5ec; border:1px solid #d4d4d4; display:inline-block; line-height:22px; margin:8px 0px 0 10px;}
.yz_qn .sj_836{ width:836px;}


/*单选题 多选题 填空题*/
.choice .title_c{color: #598900;font-size: 16px;font-weight: bold; padding-top: 15px;}
.choice dl{overflow:hidden; margin-top:10px; border-top:1px solid #eee}
.choice dl dt{font-size: 16px; line-height: 25px; padding: 12px 15px;  word-wrap: break-word; color:#000;}
.choice dl dd{overflow:hidden;font-size:16px; color:#000; text-indent:40px; line-height:35px; cursor:pointer;}
.choice dl dd .radio input{ margin-right:5px;}

.choice dl dd.wd{padding:15px 0 0 37px; background:none !important;text-indent:0; padding-bottom:0;}
.choice dl dd.wd .formTextarea{ width:97%;}

.choice dl dd.operate{ height:25px; line-height:25px;text-indent:0; background:none !important; padding-top:8px;}
.choice dl dd.operate em{ float:left; color:#2674d0; padding-left:106px; cursor:pointer;}
.choice dl dd.operate span{ float:right; padding-right:13px; display:none; }
.choice dl dd.operate span a{ float:left; width:55px; height:22px;color:#666;line-height:20px; margin-right:2px;background:#e1e4e6; font-size:14px;}
.choice dl dd.operate span a i{ width:20px; height:22px; float:left;}
.choice dl dd.operate span a i.setTop{  background:url(../images/icon/sy_icon.png) no-repeat center center;}/*上移*/
.choice dl dd.operate span a i.cancelTop{background:url(../images/icon/xy_icon.png) center center no-repeat;}/*下移*/
.choice dl dd.operate span a i.batch_revise{background:url(../images/icon/xg_icon.png) center center no-repeat;}/*修改*/
.choice dl dd.operate span a i.delete{background:url(../images/icon/sc_icon.png) center center no-repeat;}/*删除*/
.choice dl dd.operate span a.no_cz{ color:#aaa;}
.choice dl dd.operate span a.no_cz i.batch_revise{background:url(../images/icon/xgno_icon.png) center center no-repeat;}/*不可操作*/

.choice dl dd .left_545{float:left; width:50%; overflow:hidden;}
.choice dl dd .right_455{ float:left; width:50%; overflow:hidden; font-size: 16px;line-height: 35px;}
.choice dl dd .right_455 em{ padding:0 10px;color:#000}

.choice dl.add_xz{ background:#fcfcfc}
.choice dl.add_xz .formText{ width:80%; border:none; border-bottom:1px solid #e1e1e1; background:none}
.choice dl.add_xz .formTextarea{ float:left; width:79%}
.choice dl.add_xz dd{ background:none !important; text-indent:50px; }
.choice dl.add_xz dd label{text-indent:0px;}
.choice dl.add_xz .operate{ padding-bottom:8px;text-indent:0px;}
.choice dl.add_xz .operate span{ display:block;}
.sj_836 dl:hover .operate span{ display:block;}
.choice dl font{ color:#2674d0; padding-left:5px;}
.cont_addr{border:1px solid #fcc683;background:#fefcf7;padding:10px; margin-top:10px;}
.cont_addr select {  margin-left:5px}
/*td_person*/
.td_person{width: auto;height:72px;line-height: 20px; border-top:1px solid #999;border-right:1px solid #e1e1e1;border-bottom:1px solid #e4e4e4;border-left:1px solid #999; overflow-y: auto; }
.td_person ol{padding:3px 0 0 3px;overflow:hidden;}
.td_person ol li{float: left; font-size: 12px; font-weight: normal; padding-left:5px; margin-right: 3px; margin-bottom: 3px; height: 18px; line-height: 18px; border: #e5ebf0 solid 1px; background-color: #E8EBF0;word-break:keep-all; white-space:nowrap;}
.td_person ol li a.x{display:inline-block; width:10px; height: 10px;margin-left: 5px; margin-right: 5px; background: url(../images/x.gif) no-repeat; vertical-align: middle;}
.more_info{display:none;}
/*提示消息*/
.ts_message{border:1px solid #e1e1e1; overflow:hidden; margin-top:15px;}
.ts_message .mag{ background:#fafafa; text-align:center; padding:50px 0 50px 0;}
.ts_message .mag em{ background:url(../images/icon/success.png) no-repeat; height:90px; line-height:90px; display:inline-block; padding-left:130px;color:#008800;font-size:36px;}
.ts_message .mag p{ padding-left:20px; text-align:left; font-size:16px; color:#333; padding-left:475px;}
.ts_message .mag p a{ color:#0066cc; text-decoration:underline;}
.ts_message .btn{ height:42px; padding:22px 0 22px 0; background:#eaf0f4;}
.ts_message .buttonArea{border-top:1px solid #e1e1e1;padding:20px 0;background:#eaf0f4;}


/*外呼任务前台*/
.problem{background:#fafafa;width:100%;overflow:hidden;margin-top:20px;}
.problem li{border-bottom:1px dashed #b3c9db;padding:15px 15px 5px 15px;}
.problem li h5{color:#4e6373;font-size:16px;}
.problem li p{color:#555;padding:10px;}
.problem li .formTextarea{vertical-align:top;}
.problem li.last_pro{border-bottom:none;}
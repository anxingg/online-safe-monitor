@charset "utf-8";
/* CSS Document */
.CallHead{ height:99px; background:url(../images/head/c_headbg.png) repeat-x; position:relative; z-index:8}
.CallHead .c_top{ height:21px; line-height:21px; position:relative;width:100%}
.CallHead .c_top .c_logo{ margin-left:5px;  font-size:12px; font-weight:bold; color:#fff; float:left;padding-top:2px; overflow:hidden;}
.CallHead .c_top .c_titile{ margin-left:5px;  font-size:12px; font-weight:bold; color:#fff; padding-top:2px; overflow:hidden; float:right; margin-right:110px;}
.CallHead .c_top .c_menu{ width:60%;right:125px; text-align:right;color:#ffc000; position:absolute; top:0px;}
.CallHead .c_top .c_menu ul{ float:right;}
.CallHead .c_top .c_menu li{ height:22px;}
.CallHead .c_top .c_menu li a{ padding:0 5px; text-align:center; color:#fff; display:block; height:22px; margin-top:1px; line-height:20px;}
.CallHead .c_top .c_menu li{float:left; padding:0 3px;position:relative; _width:60px;}
.CallHead .c_top .c_menu li.current a{text-decoration:none; background:#166bae; border:1px solid #a0a09e; height:18px; line-height:18px; padding:0 4px;}
.CallHead .c_top .c_menu li.current .c_menu_s{ display:block;}
.CallHead .c_top .c_menu li.current .c_menu_s p a{ border:none; background:none;}
.c_menu_s{ background:#f0f0f0; border:1px solid #a0a09e; width:135px; position:absolute; top:22px; left:3px; z-index:999; display:none}
.c_menu_s p{ line-height:21px; margin:2px; height:21px; overflow:hidden;}
.CallHead .c_top .c_menu li .c_menu_s p a{ color:#000; display:block;padding:0 20px; line-height:21px;}
.c_menu_s p a:visited{ color:#000}
.CallHead .c_top .c_menu li .c_menu_s p a:Hover{ background:#3399fe; color:#fff;}

.CallHead .c_top .winClose{ width:85px; height:21px; overflow:hidden; position:absolute; top:0px; right:0px;}
/*.CallHead .c_top .winClose a{ display:block; height:19px;background:url(../images/head/btnClose.png) no-repeat; float:left;}
.CallHead .c_top .winClose a.sm{ background-position:0px -19px; width:38px;}
.CallHead .c_top .winClose a.sm:hover{background-position:0px 0px;}
.CallHead .c_top .winClose a.big{ background-position:-38px -19px; width:33px;}
.CallHead .c_top .winClose a.big:hover{background-position:-38px 0px;}
.CallHead .c_top .winClose a.cls{ background-position:-71px -19px; width:46px;}
.CallHead .c_top .winClose a.cls:Hover{ background-position:-71px 0px;}*/
.CallHead .c_top .winClose span{ display:block; height:19px;background:url(../images/head/btnClose.png) no-repeat; float:left; cursor:pointer;}
.CallHead .c_top .winClose span.sm{ background-position:0px -19px; width:38px;}
.CallHead .c_top .winClose span.sm:hover{background-position:0px 0px;}
.CallHead .c_top .winClose span.big{ background-position:-38px -19px; width:33px;}
.CallHead .c_top .winClose span.big:hover{background-position:-38px 0px;}
.CallHead .c_top .winClose span.cls{ background-position:-71px -19px; width:46px;}
.CallHead .c_top .winClose span.cls:Hover{ background-position:-71px 0px;}


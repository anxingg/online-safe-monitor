.userInfo{ height:29px; line-height:29px; padding-left:10px; color:#000; background:url(../images/head/userInfoBg.png) repeat-x; border-bottom:1px solid #ced3d7;}
.userInfo span{ padding:0 10px;}

/*operation*/

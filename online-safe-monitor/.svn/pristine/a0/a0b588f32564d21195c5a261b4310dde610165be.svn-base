<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 救援物资 -->
    <package name="emergencyDepartment" namespace="/emergencyDepartment" extends="struts-default">
        <!--添加或者修改应急机构-->
        <action name="saveOrUpdateEmergencyDepartment" class="cn.com.wh.emergencydepartment.action.EmergencyDepartmentAction" method="saveOrUpdateEmergencyDepartment">
        </action>
        <!-- 应急机构列表 -->
        <action name="findEmergencyDepList" class="cn.com.wh.emergencydepartment.action.EmergencyDepartmentAction" method="findEmergencyDepList">
        </action>
        <!-- 应急机构详情 -->
        <action name="loadEmergencyDepartmentDetail" class="cn.com.wh.emergencydepartment.action.EmergencyDepartmentAction" method="loadEmergencyDepartmentDetail">
        	<result name="update">/wh/logined/emergencyDepartment/addEmergency.jsp</result>
        </action>
        <!-- 删除应急机构 -->
        <action name="deleteEmergencyDepartment" class="cn.com.wh.emergencydepartment.action.EmergencyDepartmentAction" method="deleteEmergencyDepartment">
        </action>
    </package>
    
</struts>
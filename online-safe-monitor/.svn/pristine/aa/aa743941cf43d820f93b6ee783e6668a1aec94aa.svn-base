.input_input{margin:10px auto;clear:both;width:970px; padding:15px; background:#FFF;}
.input_input .title{background: url("../images/bg_gray.png") repeat-x scroll center bottom; color: #4e6374; height:35px; font-size:20px; line-height:22px; word-break:break-strict}

/*inputTable*/
.inputTable1 {width:100%; border:1px solid #e1e1e1; table-layout:fixed; word-break:break-all;}
.inputTable1 th {width:98px;line-height: 25px;padding: 10px;text-align: right;font-weight: normal;color: #4d6371;background:#eaf0f4;white-space: nowrap;border-right:1px solid #e1e1e1;border-bottom:1px solid #e1e1e1; vertical-align:top; font-size:16px;}
.inputTable1 th label{white-space: nowrap;}
.inputTable1 .title th, .input .inputTable1 .title td {text-align: left;padding-left: 5px;}
.inputTable1 td {line-height: 25px;padding:10px ;color: #666666; background:#fafafa;border-right:1px solid #e1e1e1;border-bottom:1px solid #e1e1e1; vertical-align:top; font-size:16px;}
.inputTable1 td em{ margin-left:5px; vertical-align:middle;}
.inputTable1 td a{color:#0647a8; /*vertical-align:middle;*/}
.inputTable1 td .formText{width:50%;}
.inputTable1 td .Wdate{width:120px;}
.inputTable1 td .num{ width:120px;}
.inputTable1 td .iconTree{width:50%;}
.inputTable1 td .formTextarea{width:95%;}
.inputTable1 td select{color:#000; margin-right:5px;width:54%;}
.inputTable1 .right_bdr0{border-right:none;}
.inputTable1 td.selectMore select{width:100px;}
.inputTable1 .tel{vertical-align:middle;margin-top:-5px;}
label.radio{ margin-left:5px; margin-right:8px;}
label.radio input{margin-right:3px; vertical-align:middle; margin-top:-3px;*margin-top: -1px;}
.input fieldset legend{ margin:0 3px; color:#000; font-size:14px; font-weight:bold;}
.input fieldset table{ width:90%; margin: 0 auto;}
.input .small_title{ height:30px; line-height:30px; font-size:18px; color:#4e6373; margin-top:20px;}
.input .p_title{ height:45px; text-align:center; color:#4d6371; font-size:22px;}
.tip{ color:#999; font-size:12px;}
.requireField{margin: 0 3px; color:#f60}
input.inputError,textarea.inputError{ border:1px dashed #f60; background:#fff8ef;}
.buttonArea_nobg{ margin-top:15px;  height:40px; line-height:40px; text-align:center; }
.input_input .buttonArea_nobg{ margin:0px; padding:25px 0px; border-top:none;}
.tableScroll{overflow:auto;}
.h250 { height:245px; overflow:auto}

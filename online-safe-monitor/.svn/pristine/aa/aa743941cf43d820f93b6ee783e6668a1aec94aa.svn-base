@charset "utf-8";
.myphoto{background:#fff;width: 140px; text-align: center; margin:0 auto; position:relative;}
.myphoto img{border:1px solid #ccc;max-width:100px;*width: expression(this.width > 100 ? "100" : true);max-height:130px;*height: expression(this.width > 130 ? "130" : true); margin-bottom:5px;}
.myphoto p{line-height: 20px; text-align:left; padding:0 5px; font-size:12px;}
.myphoto p a{ display:inline-block;}
.myphoto p input{ display:inline-block; border:none;}
.myphoto p .picadd{ background:url(../images/public/sctx.png) no-repeat; width:85px; height:26px; }
.myphoto p .piccls{  background:url(../images/public/clear_bt.png) no-repeat;width:60px; height:26px; margin-left:20px}
.myphoto a.picdele{ display:block; width:14px; height:14px; background:url(../images/public/picDelete.png) no-repeat; position:absolute; top:5px; right:5px; display:none}
.myphoto .uploadify{margin-left:15px}
/*会员管理 选项卡*/
.user_tab{ float:left; padding-top:4px;}

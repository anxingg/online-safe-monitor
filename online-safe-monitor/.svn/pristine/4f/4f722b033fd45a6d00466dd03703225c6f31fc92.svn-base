<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 应急演练 -->
    <package name="yjyl" namespace="/yjyl" extends="struts-default">
        <!--获取 应急演练 列表-->
        <action name="query" class="cn.com.wh.yjyl.action.YjylAction" method="query">
        </action>
     	<!--获取 应急演练 详情-->
        <action name="queryDetail" class="cn.com.wh.yjyl.action.YjylAction" method="queryDetail">
        	<result name="detail">/wh/yjyl/detail.jsp</result>
        	<result name="update">/wh/yjyl/update.jsp</result>
        </action>
        
        <!--新增修改-->
        <action name="saveorup" class="cn.com.wh.yjyl.action.YjylAction" method="saveorup">
        </action>
        
        <!--删除-->
        <action name="del" class="cn.com.wh.yjyl.action.YjylAction" method="del">
        </action>
        
        <!--查询重大危险源等级-->
        <action name="queryGrade" class="cn.com.wh.yjyl.action.YjylAction" method="queryGrade">
        </action>
    </package>
    
</struts>
		<!-- 后台留言管理列表 -->
		<action name="updataVoxMail" class="cn.com.qytx.hotline.messagemanagement.action.MsgManagenmentAction" method="updataVoxMail">
		</action>
		<!-- 下载录音-->
		<action name="downLoadFile" class="cn.com.qytx.hotline.messagemanagement.action.MsgManagenmentAction" method="downLoadFile">
		</action>
		<!--得所属地市-->
		<action name="getGroupName" class="cn.com.qytx.hotline.messagemanagement.action.MsgManagenmentAction" method="getGroupName">
		</action>
	</package>
</struts>
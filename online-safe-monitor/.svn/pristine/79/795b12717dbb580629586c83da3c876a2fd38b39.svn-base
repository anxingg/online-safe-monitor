		<action name="toDealMessage" class="cn.com.qytx.hotline.messagemanagement.action.MsgManagenmentAction" method="toDealMessage">
			<result name="success">/logined/workorderflow/jsp/newcaller.jsp?processId=${processId}</result>
		</action>
		<!-- 后台留言管理列表 -->
		<action name="getSeatMsgTable" class="cn.com.qytx.hotline.messagemanagement.action.MsgManagenmentAction" method="getSeatMsgTable">

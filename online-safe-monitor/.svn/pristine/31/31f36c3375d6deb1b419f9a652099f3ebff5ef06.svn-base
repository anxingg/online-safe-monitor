<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
    
<struts>
	<package name="statistics"  namespace="/statistics" extends="struts-default">
		<!-- 坐席考勤表action -->
		<action name="seatAttendance" class="cn.com.qytx.hotline.ivr.action.SeatAttendanceAction" method="seatAttendance">
		</action>		
		<!-- 坐席考勤表导出action -->
		<action name="exportSeatAttendanceList" class="cn.com.qytx.hotline.ivr.action.SeatAttendanceAction" method="exportSeatAttendanceList">
		</action>		
		<!-- 坐席考勤表定时插入在线时长，置闲时长，置忙时长，置忙次数 -->
		<action name="insertLoginMsiUserInfo" class="cn.com.qytx.hotline.ivr.action.SeatAttendanceAction" method="insertLoginMsiUserInfo">
		</action>		
		<!-- 获取部门 -->
		<action name="getGroupName" class="cn.com.qytx.hotline.ivr.action.MsiCalllogAction" method="getGroupName">
		</action>
		
		<!-- 坐席工作量统计页面的action -->
		<action name="seatTaskStatistics_*" class="cn.com.qytx.hotline.ivr.action.SeatTaskStatisticsAction" method="{1}">
		</action>
	</package>
</struts>
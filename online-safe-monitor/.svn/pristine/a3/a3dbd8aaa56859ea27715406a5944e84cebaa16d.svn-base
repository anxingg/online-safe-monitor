package cn.com.qytx.platform.domain;

import java.io.Serializable;

public class SeatFunction implements Serializable{

	/**
	 * 描述含义
	 */
	private static final long serialVersionUID = 1L;
	/**
	 * 封装坐席功能设置的相关信息
	 * 
	 * 2014-04-03 蔡明雪
	 */
	//是否开启三方通话 0：不开启，1：开启
	private Integer isThreeCall;
	//是否开启  通过空闲坐席 来获取第三方 0：不开启，1：开启
	private Integer freeSeatThreeCall;
	//是否开启  通过通讯录人员 来获取第三方 0：不开启，1：开启
	private Integer addressBookThreeCall;
	//是否开启  通过手动输入号码 来获取第三方 0：不开启，1：开启
	private Integer definePhoneThreeCall;
	//是否开启  通话转接  0：不开启，1：开启
	private Integer isTransferCall;
	//若开启单步转接，转接的方式 1:单步转接 2：咨询转接
	private Integer transferWay;
	//是否开启 转接给空闲坐席的功能  0：不开启 1：开启
	private Integer freeSeatTransfer;
	//是否开启 转接给通讯录人员的功能 0：不开启 1：开启
	private Integer addressBookTransfer;
	//是否开启 转接给 手动输入的号码 0：不开启 1：开启
	private Integer definePhoneTransfer;
	//是否开启 转接给 转队列的功能0：不开启 1：开启
	private Integer trunQueueTransFer;
	//是否开启 手动外呼 0：不开启 1:开启
	private Integer isOutCallManual;
	//是否开启 保持接回 0：不开启 1：开启
	private Integer isKeepOrRecall;
	//是否开启 挂断 0：不开启 1：开启
	private Integer isHangUp;
	//是否开启置忙置闲 0：不开启 1：开启
	private Integer isBusyOrIdle;
	//是否开启班长监听 0：不开启 1：开启
	private Integer isMonitor;
	//是否开启 话后处理 0：不开启 1：开启
	private Integer isAfterCallDeal;
	//开启话后处理，处理时长
	private Integer dealLength;
	
	//是否显示通讯录  0：不开启 1：开启
	private Integer isAddressBook;
	//是否显示知识库  0：不开启 1：开启
	private Integer isKnowledge;
	//是否显示公告  0：不开启 1：开启
	private Integer isNotice;
	//是否显示 新建工单  0：不开启 1：开启
	private Integer isNewWorkOrder;
	
	/**
	 * 默认构造函数
	 */
	public SeatFunction ()
	{
	    
	}
	
	//参数描述
	private String parDescribe;
	//参数项
	private String parItems;
	
	
	
	
	
	public Integer getDealLength() {
		return dealLength;
	}
	public void setDealLength(Integer dealLength) {
		this.dealLength = dealLength;
	}
	public String getParDescribe() {
		return parDescribe;
	}
	public void setParDescribe(String parDescribe) {
		this.parDescribe = parDescribe;
	}
	public String getParItems() {
		return parItems;
	}
	public void setParItems(String parItems) {
		this.parItems = parItems;
	}
	public Integer getIsThreeCall() {
		return isThreeCall;
	}
	public void setIsThreeCall(Integer isThreeCall) {
		this.isThreeCall = isThreeCall;
	}
	public Integer getFreeSeatThreeCall() {
		return freeSeatThreeCall;
	}
	public void setFreeSeatThreeCall(Integer freeSeatThreeCall) {
		this.freeSeatThreeCall = freeSeatThreeCall;
	}
	public Integer getAddressBookThreeCall() {
		return addressBookThreeCall;
	}
	public void setAddressBookThreeCall(Integer addressBookThreeCall) {
		this.addressBookThreeCall = addressBookThreeCall;
	}
	public Integer getDefinePhoneThreeCall() {
		return definePhoneThreeCall;
	}
	public void setDefinePhoneThreeCall(Integer definePhoneThreeCall) {
		this.definePhoneThreeCall = definePhoneThreeCall;
	}
	public Integer getIsTransferCall() {
		return isTransferCall;
	}
	public void setIsTransferCall(Integer isTransferCall) {
		this.isTransferCall = isTransferCall;
	}
	public Integer getTransferWay() {
		return transferWay;
	}
	public void setTransferWay(Integer transferWay) {
		this.transferWay = transferWay;
	}
	public Integer getFreeSeatTransfer() {
		return freeSeatTransfer;
	}
	public void setFreeSeatTransfer(Integer freeSeatTransfer) {
		this.freeSeatTransfer = freeSeatTransfer;
	}
	public Integer getAddressBookTransfer() {
		return addressBookTransfer;
	}
	public void setAddressBookTransfer(Integer addressBookTransfer) {
		this.addressBookTransfer = addressBookTransfer;
	}
	public Integer getDefinePhoneTransfer() {
		return definePhoneTransfer;
	}
	public void setDefinePhoneTransfer(Integer definePhoneTransfer) {
		this.definePhoneTransfer = definePhoneTransfer;
	}
	public Integer getTrunQueueTransFer() {
		return trunQueueTransFer;
	}
	public void setTrunQueueTransFer(Integer trunQueueTransFer) {
		this.trunQueueTransFer = trunQueueTransFer;
	}
	public Integer getIsOutCallManual() {
		return isOutCallManual;
	}
	public void setIsOutCallManual(Integer isOutCallManual) {
		this.isOutCallManual = isOutCallManual;
	}
	public Integer getIsKeepOrRecall() {
		return isKeepOrRecall;
	}
	public void setIsKeepOrRecall(Integer isKeepOrRecall) {
		this.isKeepOrRecall = isKeepOrRecall;
	}
	public Integer getIsHangUp() {
		return isHangUp;
	}
	public void setIsHangUp(Integer isHangUp) {
		this.isHangUp = isHangUp;
	}
	public Integer getIsBusyOrIdle() {
		return isBusyOrIdle;
	}
	public void setIsBusyOrIdle(Integer isBusyOrIdle) {
		this.isBusyOrIdle = isBusyOrIdle;
	}
	public Integer getIsMonitor() {
		return isMonitor;
	}
	public void setIsMonitor(Integer isMonitor) {
		this.isMonitor = isMonitor;
	}
	public Integer getIsAfterCallDeal() {
		return isAfterCallDeal;
	}
	public void setIsAfterCallDeal(Integer isAfterCallDeal) {
		this.isAfterCallDeal = isAfterCallDeal;
	}
	public Integer getIsAddressBook() {
		return isAddressBook;
	}
	public void setIsAddressBook(Integer isAddressBook) {
		this.isAddressBook = isAddressBook;
	}
	public Integer getIsKnowledge() {
		return isKnowledge;
	}
	public void setIsKnowledge(Integer isKnowledge) {
		this.isKnowledge = isKnowledge;
	}
	public Integer getIsNotice() {
		return isNotice;
	}
	public void setIsNotice(Integer isNotice) {
		this.isNotice = isNotice;
	}
	public Integer getIsNewWorkOrder() {
		return isNewWorkOrder;
	}
	public void setIsNewWorkOrder(Integer isNewWorkOrder) {
		this.isNewWorkOrder = isNewWorkOrder;
	}
	
	
	
	
}

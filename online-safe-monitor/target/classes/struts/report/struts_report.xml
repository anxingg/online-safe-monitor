<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
    
<struts>
	<package name="report"  namespace="/report" extends="struts-default">
		<!-- 坐席工作量统计页面的action -->
		<action name="seatTaskStatistics_*" class="cn.com.qytx.hotline.report.action.SeatTaskStatisticsAction" method="{1}">
		</action>
		<action name="reportTelephonetraffic_*" class="cn.com.qytx.hotline.report.action.TelephonetrafficAction" method="{1}"></action>
		<!--排队统计页面的action-->
		<action name="queueStatistics_*" class="cn.com.qytx.hotline.report.action.QueueStatisticsAction" method="{1}">
		</action> 
		<!-- 来电时段分布表action -->
		<action name="reportTimeFrame_*" class="cn.com.qytx.hotline.report.action.TimeFrameAction" method="{1}">
		</action>
		<!-- 来电区域分布表action -->
		<action name="reportAreaMisCallLog_*" class="cn.com.qytx.hotline.report.action.MisCallLogForAreaAction" method="{1}">
		</action>
		<!-- 跳转到工作汇总表 -->
		<action name="toWorkSummary" class="cn.com.qytx.hotline.report.action.WorkSummaryAction" method="toWorkSummary">
			<result name="success">/logined/report/workSummary.jsp</result>
		</action>
		<!-- 加载所属地市 -->
		<action name="loadCityName" class="cn.com.qytx.hotline.report.action.WorkSummaryAction" method="loadCityName">
		</action>
		<!-- 工作汇总列表 列表和导出-->
		<action name="list_*" class="cn.com.qytx.hotline.report.action.WorkSummaryAction" method="{1}">
		</action>
	</package>
</struts>
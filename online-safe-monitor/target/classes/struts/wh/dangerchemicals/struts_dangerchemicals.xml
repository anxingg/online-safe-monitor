<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 危险化学品模块 -->
    <package name="dangerchemicals" namespace="/dangerchemicals" extends="struts-default">
    	
    	<!-- 没有返回页面 -->
        <action name="companyDangerchemicals_*_*" class="cn.com.wh.dangerchemicals.action.CompanyDangerChemicals{1}Action" method="{2}">
        </action>
    	
    	<!-- 有返回页面 -->
        <action name="companyDangerchemicalsToPage_*_*" class="cn.com.wh.dangerchemicals.action.CompanyDangerChemicalsLoadAction" method="{1}">
        	<result name="success">/wh/logined/dangerChemicals/{2}.jsp</result>
        </action>
    	
    	
    	<!-- 没有返回页面 -->
        <action name="dangerchemicals_*_*" class="cn.com.wh.dangerchemicals.action.DangerChemicals{1}Action" method="{2}">
        </action>
    	
    	<!-- 有返回页面 -->
        <action name="dangerchemicalsToPage_*_*" class="cn.com.wh.dangerchemicals.action.DangerChemicalsLoadAction" method="{1}">
        	<result name="success">/wh/logined/dangerChemicals/{2}.jsp</result>
        </action>
    	
    	
    	<!-- 没有返回页面 -->
        <action name="chemicalsDirectory_*_*" class="cn.com.wh.dangerchemicals.action.ChemicalsDirectory{1}Action" method="{2}">
        </action>
    	
    	<!-- 有返回页面 -->
        <action name="chemicalsDirectoryToPage_*_*" class="cn.com.wh.dangerchemicals.action.ChemicalsDirectoryLoadAction" method="{1}">
        	<result name="success">/wh/logined/dangerChemicals/{2}.jsp</result>
        </action>
        
    </package>
    
</struts>
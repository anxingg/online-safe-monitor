<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 危险化学品模块 -->
    <package name="dangersourcesgood" namespace="/dangersourcesgood" extends="struts-default">
    	
    	<!-- 没有返回页面 -->
        <action name="dangerSourcesGood_*_*" class="cn.com.wh.zhdwxy.action.DangerSourcesGood{1}Action" method="{2}">
        </action>
    	
    	<!-- 有返回页面 -->
        <action name="dangerSourcesGoodToPage_*_*" class="cn.com.wh.zhdwxy.action.DangerSourcesGoodLoadAction" method="{1}">
        	<result name="success">/wh/zhdwxy/{2}.jsp</result>
        </action>
        
    </package>
    
</struts>
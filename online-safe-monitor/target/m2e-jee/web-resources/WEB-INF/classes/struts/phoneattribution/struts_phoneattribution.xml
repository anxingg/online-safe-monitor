<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
    
<struts>
	<package name="phoneAttribution"  namespace="/phoneAttribution" extends="struts-default">
		<!-- 分页查询列表 -->
		<action name="list_*" class="cn.com.qytx.hotline.mis.action.PhoneAttributionListAction" method="{1}">
		</action>
		<!-- 跳转到修改页面 -->
		<action name="toModiftPage" class="cn.com.qytx.hotline.mis.action.PhoneAttributionAction" method="toModiftPage">
			<result name="success">/hotline/phoneAttribution/jsp/addOrModift.jsp</result>
		</action>
		<!-- 操作（新增、删除、修改） -->
		<action name="operate_*" class="cn.com.qytx.hotline.mis.action.PhoneAttributionAction" method="{1}">
		</action>
		<!-- 根据号码段查询 -->
		<action name="findByMobileNumber" class="cn.com.qytx.hotline.mis.action.PhoneAttributionAction" method="findByMobileNumber">
		</action>
	</package>
</struts>
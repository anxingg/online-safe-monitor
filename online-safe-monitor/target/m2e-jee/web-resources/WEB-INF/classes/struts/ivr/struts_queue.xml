<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
    
<struts>
	<package name="seatQueue"  namespace="/seatQueue" extends="struts-default">
	
		<!-- 坐席队列列表的action，和修改的保存操作 -->
		<action name="queue_*" class="cn.com.qytx.hotline.ivr.action.SeatQueueAction" method="{1}"></action>
		
		<!-- 跳编辑页面的action -->
		<action name="findById" class="cn.com.qytx.hotline.ivr.action.SeatQueueFindAction" method="findById">
			<result name="success">/logined/queue/queueEdit.jsp</result>
		</action>
		
		<!-- 根据队列id获取队列说明（坐席端，转队列用） -->
		<action name="findNameById" class="cn.com.qytx.hotline.ivr.action.SeatQueueFindAction" method="findNameById"></action>
		
	</package>
</struts>
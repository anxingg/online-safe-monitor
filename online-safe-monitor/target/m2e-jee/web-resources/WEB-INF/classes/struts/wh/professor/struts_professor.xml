<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <!-- 安全生产专家管理模块 -->
    <package name="professor" namespace="/professor" extends="struts-default">
       <!-- 新增或修改 -->
       <action name="addOrUp" class="cn.com.wh.professor.action.ProfessorAction" method="addOrUp">
       </action>
       
       <!-- 跳转到新增或修改页面 -->
       <action name="getProfessorInfoAdd" class="cn.com.wh.professor.action.ProfessorAction" method="getProfessorInfo">
       	<result name="success">/wh/logined/professor/addProfessor.jsp</result>
       </action>
       
       <!-- 跳转到查看页面 -->
       <action name="getProfessorInfoView" class="cn.com.wh.professor.action.ProfessorAction" method="getProfessorInfo">
       	<result name="success">/wh/logined/professor/professorView.jsp</result>
       </action>
       
       <!-- 列表 -->
       <action name="getList" class="cn.com.wh.professor.action.ProfessorAction" method="getList">
       </action>
       
       <!-- 删除 -->
       <action name="delete" class="cn.com.wh.professor.action.ProfessorAction" method="delete">
       </action>
       
       
       
    </package>
    
</struts>